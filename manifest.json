{
  "manifest_version": 2,
  "name": "webX_msg",
  "version": "1.0",
  "description": "stress marking tool",

"permissions": [
  "http://127.0.0.1/*"
  ],

"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },
  
  "icons": {
  "48": "icons/icon-48.png",
  "96": "icons/icon-96.png"
  },
  
  "browser_action": {
   "default_icon":{
     "19":"icons/icon-19.png",
     "38":"icons/icon-38.png"
   }, 
   
   "browser_style": true,
    "default_title": "Stress marker",
    "default_popup": "popup.htm"
  },
  
  "web_accessible_resources": [
    "EDLEndianDict"
  ]

}